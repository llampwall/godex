{
  "name": "godex",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "test:server": "pnpm --filter ./apps/server test",
    "test:ui": "pnpm --filter ./apps/ui build",
    "test": "pnpm -s test:server && pnpm -s test:ui"
  }
}
